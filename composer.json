        "illuminate/contracts": "5.3.*",
        "illuminate/http": "5.3.*",
        "illuminate/support": "5.3.*",
        "nesbot/carbon": "~1.20"
    },
    "require-dev": {
        "orchestra/testbench": "3.3.*",
        "phpunit/phpunit": "~5.0",
        "phpspec/phpspec": "^2.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
