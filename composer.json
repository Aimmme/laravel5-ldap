    "description": "Ldap (Active Directory) authentication for Laravel 5",
    "keywords": ["laravel", "ldap", "windows", "authenticate", "active directory"],
    "license" : "MIT",
    "authors": [
        {
            "name": "Mohamed Aiman",
            "email": "muhammadhu.aiman@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
